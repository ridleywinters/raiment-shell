{
    "imports": {
        "react": "npm:react@19",
        "react/jsx-runtime": "npm:react@19/jsx-runtime",
        "react/jsx-dev-runtime": "npm:react@19/jsx-dev-runtime",
        "react-dom/client": "npm:react-dom@19/client"
    },
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "types": [
            "npm:@types/react@19",
            "npm:@types/react-dom@19"
        ],
        "strict": true,
        "lib": [
            "deno.ns",
            "dom",
            "dom.iterable",
            "esnext"
        ]
    },
    "fmt": {
        "useTabs": false,
        "indentWidth": 4,
        "lineWidth": 100
    }
}
